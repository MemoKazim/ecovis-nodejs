<%-include("partials/header" , {title: title});-%>
<body>
  <%-include("partials/navigator" , {title: title});-%>
  <form
    method="post"
    enctype="multipart/form-data"
    action="/admin/uploadPartner"
  >
    <label for="name">Name: </label>
    <input type="text" name="name" id="name" /> <br />

    <label for="website">Website: </label>
    <input type="text" name="website" id="website" /> <br />

    <label for="detail">About Partner: </label>
    <textarea
      type="text"
      name="detailsAZ"
      id="detail"
      placeholder="AZ"
    ></textarea>
    <textarea
      type="text"
      name="detailsEN"
      id="detailEN"
      placeholder="EN"
    ></textarea>
    <br />

    <label for="image">Image: </label>
    <input type="file" name="uploadedImage" id="image" required /> <br />
    <button type="submit">Send</button>
  </form>
</body>
